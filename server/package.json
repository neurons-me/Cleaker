{
  "name": "api.cleaker.me",
  "version": "1.0.1",
  "description": "Minimal identity + semantics node server.",
  "author": "suiGn",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "dev": "tsx watch server.ts",
    "build": "tsc",
    "start": "tsx server.ts",
    "lint": "eslint . --ext .ts",
    "runBuild": "npm run build && npm run start",
     "docs:api": "typedoc --entryPoints ./server.ts --out docs/api --plugin typedoc-plugin-markdown --readme none --excludePrivate --excludeInternal",
    "docs:api:watch": "typedoc --watch --entryPoints ./server.ts --out docs/api --plugin typedoc-plugin-markdown --readme none --excludePrivate --excludeInternal"
  },
  "dependencies": {
    "better-sqlite3": "^12.5.0",
    "cleaker": "^2.6.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "os": "^0.1.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6",
    "typedoc": "^0.28.16",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.9.3",
    "vitepress": "^1.6.4"
  }
}
